{
  "extends": [
    "development"
  ],
  "hints": {
    "compat-api/html": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "font-synthesis",
          "-webkit-text-size-adjust"
        ]
      }
    ],
    "no-inline-styles": "off",
    "axe/name-role-value": [
      "default",
      {
        "button-name": "off",
        "link-name": "off"
      }
    ],
    "axe/forms": "off",
    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off"
      }
    ],
    "meta-viewport": "off",
    "axe/language": "off",
    "axe/aria": "off",
    "axe/structure": "off"
  }
}